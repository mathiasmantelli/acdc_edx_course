<?xml version="1.0"?>
<package format="2">
    <name>object_fusion_wrapper</name>
    <version>0.1.0</version>
    <description>
        ROS-wrapper for usage of the object_fusion package as a ROS node.
    </description>

    <maintainer email="acdc@ika.rwth-aachen.de">ACDC</maintainer>

