<?xml version="1.0"?>
<package format="2">
  <name>camera_based_semantic_grid_mapping</name>
  <version>0.1.0</version>
  <description>This package performs semantic grid mapping using inverse perspective mapping on segmented camera images.</description>

  <maintainer email="acdc@ika.rwth-aachen.de">ACDC</maintainer>

  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>
 
